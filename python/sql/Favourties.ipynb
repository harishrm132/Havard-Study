    "import csv\n",
    "\n",
    "counter = 0\n",
    "with open(\"Favourties.csv\", \"r\") as file:\n",
    "    reader = csv.DictReader(file)\n",
    "    for row in reader:\n",
    "        t = row[\"title\"].strip().upper()\n",
    "        if t == \"THE OFFICE\":\n",
    "            counter += 1\n",
    "print(f\"No of people who likes OFFICE: {counter}\")"
   ]
  }
